create database air_p;

show databases;

use air_p;

create external table airport (
id int,
name string,
city string,
country string,
i_f varchar(3),
icao varchar(4),
lat decimal(10,6),
lon decimal(10,6),
alt int,
timez decimal(3,1),
dst varchar(2),
tz string)
row format delimited
fields terminated by ','
stored as textfile
location '/hive_ex/airport';


load data local inpath '/home/ankita/airports_mod.dat' 
overwrite into table airport;


create external table routes(
airline varchar(3),
a_id int,
s_a varchar(4),
s_a_id int,
d_a varchar(4),
d_a_id int,
codeshare varchar(2),
stops int,
equipment string)
row format delimited
fields terminated by ','
stored as textfile
location '/hive_ex/routes';


load data local inpath '/home/ankita/routes.dat' 
overwrite into table routes;



create external table airline(
airline int,
name string,
alias string,
i_t varchar(2),
icao varchar(3), 
callsign string,
country string,
active varchar(2)
)
row format delimited
fields terminated by ','
stored as textfile
location '/hive_ex/airline';

load data local inpath '/home/ankita/Final_airlines' 
overwrite into table airline;

